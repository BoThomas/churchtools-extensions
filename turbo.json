{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "type-check": {
      "dependsOn": ["^type-check"]
    },
    "deploy": {
      "dependsOn": ["build"],
      "outputs": ["../../releases/**"],
      "cache": false
    },
    "format": {
      "outputs": [],
      "cache": false
    }
  }
}
